<div class="col-md-12">
  <div class="movie">
    <%= link_to("电影列表", new_account_movie_path, class: "btn btn-default pull-right") %>
  </div>
  <table class="table table-hover">
    <thead>
      <tr>
        <td>电影名</td>
        <td>简介</td>
        
      </tr>
    </thead>
    <tbody>
      <% @movies.each do |movie| %>
      <tr>
        <td><%= link_to(movie.name, account_movie_path(movie)) %></td>
        <td><%= movie.review %></td>

      </tr>
      <% end %>
    </tbody>
  </table>
</div>
